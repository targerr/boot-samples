<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.DocumentMapper">

    <resultMap id="BaseResultMap" type="com.example.pojo.Document">
            <id property="id" column="id" jdbcType="VARCHAR"/>
            <result property="name" column="name" jdbcType="VARCHAR"/>
            <result property="details" column="details" jdbcType="VARCHAR"/>
            <result property="description" column="description" jdbcType="VARCHAR"/>
            <result property="createDateTime" column="create_date_time" jdbcType="TIMESTAMP"/>
            <result property="createName" column="create_name" jdbcType="VARCHAR"/>
            <result property="modifyDateTime" column="modify_date_time" jdbcType="TIMESTAMP"/>
            <result property="modifyName" column="modify_name" jdbcType="VARCHAR"/>
            <result property="isDelete" column="is_delete" jdbcType="BIT"/>
            <result property="type" column="type" jdbcType="INTEGER"/>
            <result property="state" column="state" jdbcType="INTEGER"/>
            <result property="label" column="label" jdbcType="VARCHAR"/>
            <result property="sorting" column="sorting" jdbcType="INTEGER"/>
            <result property="version" column="version" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        id,name,details,
        description,create_date_time,create_name,
        modify_date_time,modify_name,is_delete,
        type,state,label,
        sorting,version
    </sql>
</mapper>
